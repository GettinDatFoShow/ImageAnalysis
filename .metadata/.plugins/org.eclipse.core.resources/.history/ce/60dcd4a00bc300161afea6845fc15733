package application;

public class ImageRecord {

	private String filepath;
	private String filename;
	private String date;
	private String red;
	private String green;
	private String blue;
	private String colorfulness;
	
	public ImageRecord(){
		String filepath;
		String filename;
		String date;
		String red;
		String green;
		String blue;
		String colorfulness;
	}
	
	public ImageRecord(String fp, String fn, String d, String r, String g, String b, String c){
		String filepath = fp;
		String filename = fn;
		String date = d;
		String red = r;
		String green = g;
		String blue = b;
		String colorfulness = c;
	}
	
	public String getFilepath() {
		return filepath;
	}
	public void setFilepath(String filepath) {
		this.filepath = filepath;
	}
	public String getFilename() {
		return filename;
	}
	public void setFilename(String filename) {
		this.filename = filename;
	}
	public String getDate() {
		return date;
	}
	public void setDate(String date) {
		this.date = date;
	}
	public String getRed() {
		return red;
	}
	public void setRed(String red) {
		this.red = red;
	}
	public String getGreen() {
		return green;
	}
	public void setGreen(String green) {
		this.green = green;
	}
	public String getBlue() {
		return blue;
	}
	public void setBlue(String blue) {
		this.blue = blue;
	}
	public String getColorfulness() {
		return colorfulness;
	}
	public void setColorfulness(String colorfulness) {
		this.colorfulness = colorfulness;
	}
}
